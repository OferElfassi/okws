#!/bin/sh
set -e -u

# A bunch of the AC_SUBST contain other other bash variables. Before we use
# okwsbindir we need to make sure all those variables are in the environment
SHORTVERSION="@SHORTVERSION@"
prefix="@prefix@"
exec_prefix="@exec_prefix@"
libdir="@libdir@"
pkglibdir="${libdir}/@PACKAGE@"
okwstagdir="@okwstagdir@"

cat<<EOF
""" This module contains variable that were set when compiling okws. It is
autogenerated by okws's build system."""
SHORTVERSION    = '@SHORTVERSION@'
VERSION    = '@VERSION@'
BINDIR = '@okwsbindir@'
EOF
